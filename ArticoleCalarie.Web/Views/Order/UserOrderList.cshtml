@model PagedList.IPagedList<ArticoleCalarie.Models.OrderViewModel>
<link href="~/Content/PagedList.css" rel="stylesheet" type="text/css" />

@{
    ViewBag.Title = "Comenzile Mele";
    ViewBag.MetaDescription = "Comenzile mele efectuate pe articole-calarie.ro.";
}

@section MetaNoFollow {
    <meta name='robots' CONTENT='NOINDEX, NOFOLLOW'>
}


@Html.Partial("_AccountHeaderPartial")

@Html.Partial("_UserOrderListPartial", Model)


@section scripts {
    <script src="~/Scripts/user-orders.js"></script>
    <script>
            $(document).ready(function () {
                getRelatedProducts('#orderList-related-products');
            });
    </script>
}

